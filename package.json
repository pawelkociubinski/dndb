{
  "name": "ddb",
  "version": "1.0.0",
  "description": "",
  "main": "/dist/index.js",
  "type": "module",
  "author": "Paweł Kociubiński",
  "scripts": {
    "build": "tsc",
    "start": "node ./dist/index.js --enable-source-maps=true",
    "develop": "open-cli ./public/index.html -- 'google chrome' && nodemon ./src/index.ts",
    "test": "jest --collectCoverage true",
    "test:watch": "jest --watch --collectCoverage false --detectOpenHandles",
    "generate": "graphql-codegen",
    "knex:run": "npm run knex",
    "knex:migrate:make": "ts-node-esm ./node_modules/.bin/knex migrate:make --",
    "knex:migrate:latest": "ts-node-esm ./node_modules/.bin/knex migrate:latest --debug",
    "knex:migrate:rollback": "ts-node-esm ./node_modules/.bin/knex migrate:rollback --all --debug"
  },
  "dependencies": {
    "@apollo/server": "^4.9.5",
    "@graphql-tools/schema": "^10.0.0",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "graphql": "^16.8.1",
    "graphql-middleware": "^6.1.35",
    "graphql-scalars": "^1.22.4",
    "knex": "^3.0.1",
    "lodash": "^4.17.21",
    "pino": "^8.16.1",
    "pino-http": "^8.5.1",
    "sqlite3": "^5.1.6",
    "uuid": "^9.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/client-preset": "^4.1.0",
    "@graphql-codegen/typescript-resolvers": "^4.0.1",
    "@types/body-parser": "^1.19.4",
    "@types/cors": "^2.8.15",
    "@types/express": "^4.17.20",
    "@types/jest": "^29.5.7",
    "@types/lodash": "^4.14.200",
    "@types/node": "^20.8.10",
    "@types/sqlite3": "^3.1.10",
    "@types/uuid": "^9.0.6",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-n": "^16.2.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "open-cli": "^7.2.0",
    "pino-pretty": "^10.2.3",
    "prettier": "^3.0.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  }
}
